	Day = {01},
	Doi = {10.1007/s00035-017-0194-x},
	Issn = {1664-221X},
	Journal = {Alpine Botany},
	Month = {Apr},
	Number = {1},
	Pages = {71--81},
	Title = {Seed dormancy and longevity in subarctic and alpine populations of Silene suecica},
	Url = {https://doi.org/10.1007/s00035-017-0194-x},
	Volume = {128},
	Year = {2018},
	Bdsk-Url-1 = {https://doi.org/10.1007/s00035-017-0194-x}}

