	Issn = {2211-3460},
	Journal = {Horticulture, Environment, and Biotechnology},
	Month = {Jun},
	Number = {3},
	Pages = {366--375},
	Title = {Non-deep simple morphophysiological dormancy in seeds of Thalictrum rochebrunianum, an endemic perennial herb in the Korean Peninsula},
