	Doi = {10.1007/s10725-015-0127-x},
	Issn = {1573-5087},
	Journal = {Plant Growth Regulation},
	Month = {Jun},
	Number = {2},
	Pages = {219--227},
	Title = {Manipulation of cold stratification and endophytic effects on expression patterns of RSG and KAO genes in coleorhiza of wheat seeds},
